package day1;

public class Positive 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int num=-90;  
		if(num>0)  
		{  
		System.out.println("The number is +ve");  
		}    
		else if(num<0)  
		{  
		System.out.println("The number is -ve");  
		}    
		else  
		{  
		System.out.println("The number is zero");  
		}  
	}

}
